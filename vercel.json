{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/**": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_NAME": "NeonHub v2.3.3",
    "NEXT_PUBLIC_VERSION": "2.3.3"
  },
  "regions": ["iad1"],
  "github": {
    "autoDeployment": true
  }
}
